daemonize no
pidfile /var/vcap/sys/run/redis/redis.pid
port <%= p("bps.redis.port") %>
<% if_p("bps.redis.listen_address") do |listen_address| %>
bind <%= listen_address %>
<% end %>
timeout 300
loglevel notice
logfile ""
databases 16
dir /var/vcap/data/redis
requirepass <%= p("bps.redis.password") %>
maxmemory-policy noeviction
appendonly no

